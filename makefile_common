#Definiciones de targets.

all: \
$(DIR_O)dummy \
$(EJECUTABLE) \

$(DIR_O)dummy:
	$(PREPARAR_DIR_OBJECTS)

$(EJECUTABLE): \
$(DIR_LIBDAN2)/libDanSDL2.a\
$(DEP_APP) \
$(DEP_FRAMEWORK) \
$(DEP_CONTROLLERS) \
$(DEP_TOOLS) \
main.cpp
	$(CC) $(CFLAGS_TARGET_FINAL) $(INCLUDES) main.cpp $(DEP_FRAMEWORK) $(DEP_APP) $(DEP_CONTROLLERS) $(DEP_TOOLS) -o $(EJECUTABLE) $(LIBRERIAS) $(LINKER)

clean:
	$(LIMPIAR)
